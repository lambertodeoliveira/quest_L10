{
    "name": "caneara/quest",
    "description": "A package that provides pseudo fuzzy-searching to Laravel database queries.",
    "keywords": [
        "quest",
        "php",
        "laravel",
        "search",
        "fuzzy"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/caneara/quest",
    "autoload": {
        "psr-4": {
            "Quest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Quest\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.3",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Quest\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
